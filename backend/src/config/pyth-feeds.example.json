{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Pyth Price Feed IDs for Base Network",
  "description": "Mapping of asset symbols to Pyth Network price feed IDs",
  "type": "object",
  "properties": {
    "feeds": {
      "type": "object",
      "description": "Symbol to Pyth feed ID mappings",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "feedId": {
            "type": "string",
            "description": "Pyth price feed ID (hex string with 0x prefix)",
            "pattern": "^0x[0-9a-f]{64}$"
          },
          "description": {
            "type": "string",
            "description": "Human-readable description of the feed"
          },
          "source": {
            "type": "string",
            "description": "Original feed name or type (e.g., 'ETH/USD', 'BTC/USD')"
          },
          "isProxy": {
            "type": "boolean",
            "description": "True if this symbol uses another asset's feed as a proxy"
          }
        },
        "required": ["feedId", "description"]
      }
    }
  },
  "feeds": {
    "WETH": {
