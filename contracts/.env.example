# Contracts Environment Variables
# Copy this file to .env and fill in the values

# RPC URL for Base network
RPC_URL=https://mainnet.base.org

# Private key for contract deployment (keep secure!)
# Remove 0x prefix if present
EXECUTION_PRIVATE_KEY=your_private_key_here

# BaseScan API key for contract verification
BASESCAN_API_KEY=your_basescan_api_key_here

# Contract addresses (after deployment)
# EXECUTOR_ADDRESS=0x...

# For forking Base network in tests
# FORK_BLOCK_NUMBER=12345678
